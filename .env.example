# Supabase Configuration
# Replace these placeholder values with your actual Supabase project credentials
# You can find these values in your Supabase project settings under "API"

VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Example format:
# VITE_SUPABASE_URL=https://abcdefghijklmnop.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# AI Model Configuration
# Primary AI Provider: 'perplexity' or 'deepseek'
PRIMARY_AI_PROVIDER=perplexity

# Perplexity API Configuration
PERPLEXITY_API_KEY=your_perplexity_api_key_here
# Available Perplexity models: sonar-reasoning, llama-3.1-sonar-large-128k-online, llama-3.1-sonar-small-128k-online
PERPLEXITY_MODEL=sonar-reasoning

# DeepSeek API Configuration
DEEPSEEK_API_KEY=sk-ff0f3f69ba9e4ef8949b201ccba7f1a1
DEEPSEEK_MODEL=deepseek-reasoner

# External API Keys
# Historical Data API Key
HISTORICAL_DATA_API_KEY=your_historical_data_api_key_here

# Support & Resistance API Key
SUPPORT_RESISTANCE_API_KEY=your_support_resistance_api_key_here

# Example format:
# PRIMARY_AI_PROVIDER=perplexity
# PERPLEXITY_API_KEY=pplx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# PERPLEXITY_MODEL=sonar-reasoning
# DEEPSEEK_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# DEEPSEEK_MODEL=deepseek-reasoner
# HISTORICAL_DATA_API_KEY=lmt_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# SUPPORT_RESISTANCE_API_KEY=your_api_key_here